package pack1;

public class Hello {

	public static void main(String[] args) {
		System.out.println("Greatful things");
		System.out.println("djkfhdskjfh");
		System.out.println("*  * *** *     *     ***");
		System.out.println("*  * *   *     *    *   *");
		System.out.println("**** *** *     *    *   *");
		System.out.println("*  * *   *     *    *   *");
		System.out.println("*  * *** ****  ****  ***");
		int i = 4;
		char c = 'd';
		boolean isTrue = true;
		long l = 9999999;
		short s = 45;
		byte b = -128;
		double d = 23.5;
		float f = 45.6f;
		float t = (float) 45.6;

		if (s < b) {
			System.out.println(s);
		} else if (isTrue) {
			System.out.println(b);
		}

		switch (i) {
		case 1: {
			System.out.println("case 1");
			System.out.println(i);
			break;
		}
		case 2: {
			System.out.println("case 2");
			System.out.println(i);
			break;
		}
		default: {
			System.out.println("default");
			System.out.println(i);
		}
		}
		System.out.println("start while");
		while(i<10){
			i++;
			System.out.println(i);
		}
		int j=4;
		do{
			j++;
			System.out.println(j);}
		while (j<10);
			
		System.out.println("Start for");
		for(int ij=0; ij<10; ij++){
			System.out.println(ij);
		}
	}
	// System.out.println для виводу стрічкових елементів;
	// public static void main(String[] args) - main метод точка входу в;
	// ctrl shift f для формат коду по вiдступах;
	// System.out.println(); - syso -ctrl-space;
	// default якшо не спрацбовуэ жодний case вкінці його не треба break;
	//switch швидше за if;
	//while виконуэться поки умова ыстинна
	//do while виконуэться якшо треб  шоб викон умова хоч раз
	//for цикл для виконання пвеної кількості раз до виконння умови
}

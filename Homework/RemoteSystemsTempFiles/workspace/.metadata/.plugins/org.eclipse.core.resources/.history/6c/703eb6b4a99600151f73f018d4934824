package hometask1;

public class First {
	static double sqr(int b) {
		return sqr(b);}
	static double sqrt(double d) {
		return sqrt(d);}
		
		
	public static void main(String[] args, Object x1) {
		int a = 8;
		int b = 4;
		int c = 2;
		
		x1=((sqrt(sqr(b)-4*a*c))-b)/(2*a);
		System.out.println(x1);
	}

	
	}



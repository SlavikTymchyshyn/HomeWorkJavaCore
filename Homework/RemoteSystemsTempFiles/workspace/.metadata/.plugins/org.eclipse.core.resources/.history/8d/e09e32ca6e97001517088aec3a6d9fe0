package less2;

public class Main {

	public static void main(String[] args) {
		int[] array = new int[50];
		array[0]=1;
		array[1]=2;
		array[2]=2;
		array[3]=2;
		for(int i =0;i <10; i++){array[i]=i+1;
		}
		for(int i =0;i <10; i++){
		System.out.println(array[i]);}
		
		char[] massiv = new char[3];
		array[0]='r';
		array[1]='j';
		array[2]='h';
		System.out.println(massiv);
	}

}

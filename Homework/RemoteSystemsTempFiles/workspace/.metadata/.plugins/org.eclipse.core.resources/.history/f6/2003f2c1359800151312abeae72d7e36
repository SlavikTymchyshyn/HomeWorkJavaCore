package home2;

public class Task1simple {

	public static void main(String[] args) {
		int[] ar1=new int[10];
		for (int j = 0; j < 10; j++) {ar1[j] = j*2+2;System.out.print(ar1+" ");}
		System.out.println();
		for (int j = 0; j < 10; j++) {ar1[j] = j*2+2;System.out.println(ar1[j]);}
		}
	
}

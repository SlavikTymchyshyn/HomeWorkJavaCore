package less2;

public class Main {

	public static void main(String[] args) {
		int[] array = new int[50];
		array[0] = 1;
		array[1] = 2;
		array[2] = 2;
		array[3] = 2;
		for (int i = 0; i < 10; i++) {
			array[i] = i + 1;
		}
		for (int i = 0; i < 10; i++) {
			System.out.println(array[i]);
		}

		char[] mass = new char[3];
		mass[0] = 'r';
		mass[1] = 'j';
		mass[2] = 'h';
		for (int i = 0; i < 3; i++) {
			System.out.println(mass);
		}

		// 2вимірний масив внизу
		int[][] twoArray = new int[11][11];
		for (int i = 0; i < 11; i++) {
			for (int j = 0; j < 11; j++) {
				twoArray[i][j] = i * j;
			}
		}
		for (int i = 0; i < 11; i++) {
			for (int j = 0; j < 11; j++) {
				if (twoArray[i][j] < 11) {
					System.out.print(twoArray[i][j] + "  ");
				} else {
					System.out.print(twoArray[i][j] + " ");
				}
			}
			System.out.println();
		}
	}
}


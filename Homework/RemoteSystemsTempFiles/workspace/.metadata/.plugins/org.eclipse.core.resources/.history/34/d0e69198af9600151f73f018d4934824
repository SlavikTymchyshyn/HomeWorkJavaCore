package hometask1;

public class First {

	public static void main(String[] args) {
		double a=8;
		double b=8;
		double c=8;
		double x1;
		double x2;

		x1 = (Math.sqrt(Math.pow(b, 2) - 4 * a * c) - b) / (2 * a);
		x2 = (Math.sqrt(Math.pow(b, 2) - 4 * a * c) + b) / (2 * a);

		System.out.println("No result");
		System.out.println("x1 = " + x1);
		System.out.println("x2 = " + x2);

	}
}
	



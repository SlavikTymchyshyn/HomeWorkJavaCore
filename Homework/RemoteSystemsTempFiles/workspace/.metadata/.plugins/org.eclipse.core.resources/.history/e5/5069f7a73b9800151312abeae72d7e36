package home2;

public class Twoarray {

	public static void main(String[] args) {
		int twoar[][] =new int[10][20];
		for (int i=9; i>-1;i--){for (int j=0; j<20;j++){twoar[i][j]=-(2*i+1)+(2*j+2);
		if (twoar[i][j]>10){System.out.print(twoar[i][j]+" ");}
		else{System.out.println(twoar[i][j]+"  ");}
		System.out.println();
		}}}}

		


package pack1;

public class Hello {

	public static void main(String[] args) {
		System.out.println("Greatful things");
		System.out.println("djkfhdskjfh");
		System.out.println("*  * *** *     *     ***");
		System.out.println("*  * *   *     *    *   *");
		System.out.println("**** *** *     *    *   *");
		System.out.println("*  * *   *     *    *   *");
		System.out.println("*  * *** ****  ****  ***");
		int i = 2;
		char c = 'd';
		boolean isTrue = true;
		long l = 9999999;
		short s = 45;
		byte b = -128;
		double d = 23.5;
		float f = 45.6f;
		float t = (float)45.6;
		
		if(s<b){
			System.out.println(s);
		}else if (isTrue) {System.out.println(b);
	}
	
	switch (i){
	case 1 :{System.out.println(i);
	break;}
	case 2 :{System.out.println(i);
	break;
	}
	default :{System.out.println(i);}
	}
	}
	// System.out.println для виводу стрічкових елементів
	// public static void main(String[] args) - main метод точка входу в
	// ctrl shift f для формат коду по вiдступах
	//System.out.println(); - syso -ctrl-space
	//default  якшо не спрацбовуэ жодний case вкінці його не треба break;
}
